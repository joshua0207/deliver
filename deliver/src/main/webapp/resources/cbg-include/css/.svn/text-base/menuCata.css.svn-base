@charset "UTF-8";
.addFormBtn{
	height:27px;
	margin-bottom:10px;
	}
.addFormBtn ul{
	margin:0;
	padding:0;
	}	
.addFormBtn ul li{
	margin:0;
	list-style:none;
	width:61px;
	height:27px;
	padding-right:10px;
	float:left;
	}
.addpro_Btn{
	width:61px;
	height:27px;
	display:block;
	text-indent:-9999px;
	background-image:url(/cbg-include/images/menu/addpro_Btn.png);
	background-repeat:no-repeat;
	border:none 0;
	cursor:pointer;
	margin-bottom:5px;
	}
.delate_Btn{
	width:61px;
	height:27px;
	display:block;
	text-indent:-9999px;
	background-image:url(/cbg-include/images/menu/delate_btn.png);
	background-repeat:no-repeat;
	border:none 0;
	cursor:pointer;
	margin-bottom:5px;
	}
