.editWord{
	color:#000;
	font-size:12px;
	}
.editWord a{
	color:#000;
	text-decoration:underline;	
	}
.editWord a:hover{
	color:#000;
	text-decoration:none;	
	}
.bor{border-bottom:#ffceab 1px solid;}

